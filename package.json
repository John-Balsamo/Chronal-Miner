{
 "name": "ChronalMiner",
 "productName": "Chronal Miner",
 "version": "1.0.1",
 "main": "main.js",
 "devDependencies": {
 "electron": "^1.4.3",
 "electron-packager": "^8.1.0"
 },
 "scripts": {
"package-win": "electron-packager . ChronalMiner --overwrite --asar=true --platform=win32 --arch=ia32  --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Chronal Miner\"",    
"package-linux": "electron-packager . ChronalMiner --overwrite --asar=true --platform=linux --arch=x64  --prune=true --out=release-builds"
 }
}